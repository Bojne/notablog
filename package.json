{
  "name": "notablog",
  "version": "0.3.11",
  "description": "Generate a minimalistic blog from a Notion table.",
  "author": "dragonman225",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:module && npm run build:doc",
    "build:module": "rm -rf dist && rollup -c && tsc --emitDeclarationOnly",
    "build:doc": "npm run build:typedoc && npm run build:dependency-graph",
    "build:typedoc": "typedoc --out typedoc --mode file src/",
    "build:dependency-graph": "npx depcruise --exclude '^(node_modules|src/interfaces)' --output-type dot --prefix 'https://github.com/dragonman225/notablog/tree/master/' src/index.ts | dot -T svg > documentation/dependency-graph.svg",
    "test": "INDENT=true ts-node test/index.spec.ts",
    "release": "npm run build && npm publish",
    "upgrade": "node tools/upgrade-deps.js"
  },
  "dependencies": {
    "@dnpr/fsutil": "^0.1.3",
    "@dnpr/logger": "^0.2.0",
    "@dnpr/task-manager": "^0.1.2",
    "notionapi-agent": "^1.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^2.0.1",
    "@types/node": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "dependency-cruiser": "^6.1.0",
    "eslint": "^6.8.0",
    "rollup": "^1.27.14",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "zora": "^3.1.8"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "nodejs",
    "notion",
    "blog"
  ],
  "homepage": "https://github.com/dragonman225/notablog",
  "repository": {
    "type": "git",
    "url": "https://github.com/dragonman225/notablog.git"
  },
  "bugs": {
    "url": "https://github.com/dragonman225/notablog/issues"
  },
  "typeScriptVersion": "3.7",
  "runkitExampleFilename": ""
}
